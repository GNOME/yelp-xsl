<tmpl>
div.note-version {
  background-color: {{$color.bg.yellow}};
  margin-top: 1em;
  margin-bottom: 1em;
}
span.status {
  font-size: 0.83em;
  font-weight: normal;
  padding-left: 0.2em;
  padding-right: 0.2em;
  color: {{$color.fg.dark}};
  border: solid 1px {{$color.red}};
  background-color: {{$color.bg.yellow}};
}
span.status-stub, span.status-draft, span.status-incomplete, span.status-outdated {
  background-color: {{$color.bg.red}};
}
